pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'Test Jenkins..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'Test Jenkins again..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'Test Jenkins again and again..'
            }
        }
    }
}
